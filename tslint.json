{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "semicolon": [true, "never"],
        "quotemark": [true, "single"],
        "no-var-requires": false,
        "no-angle-bracket-type-assertion": false,
        "trailing-comma": [],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}